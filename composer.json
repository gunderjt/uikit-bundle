{
    "name":         "gunderjt/uikit-bundle",
    "description":  "GunderjtUikitBundle integrates Uikit into Symfony by providing templates, Twig extensions, services and commands.",
    "keywords":     ["uikit"],
    "type":         "symfony-bundle",
    "license":      "MIT",
    "authors":     [
        {
            "name":     "Jeffrey Gunderson",
            "email":    "jeffrey.gunderson@gmail.com",
        }
    ],
    "require":      {
        "php":                      ">=5.4",
        "symfony/framework-bundle": "~2.6|~3.0",
        "symfony/twig-bundle":      "~2.6|~3.0",
        "symfony/form":             "~2.6|~3.0",
        "symfony/console":          "~2.6|~3.0",
        "symfony/finder":           "~2.6|~3.0",
        "twig/twig":                "~1.12|~2.0@dev"
    },
    "require-dev": {
        "symfony/assetic-bundle":       "~2.6|~3.0",
        "knplabs/knp-menu":             "~2.0@alpha",
        "knplabs/knp-menu-bundle":      "~2.0@alpha",
        "knplabs/knp-paginator-bundle": "dev-master",
        "phpunit/phpunit":              "~3.7",
        "mockery/mockery":              "~0.9"
    },
    "suggest":      {
        "knplabs/knp-paginator-bundle": "UikitBundle styles the pagination provided by KnpPaginatorBundle.",
        "knplabs/knp-menu":             "Required to use KnpMenuBundle.",
        "knplabs/knp-menu-bundle":      "BraincraftedBootstrapBundle styles the menus provided by KnpMenuBundle.",
        "uikit/uikit":                  "Uikit provides the assets"
    },
}